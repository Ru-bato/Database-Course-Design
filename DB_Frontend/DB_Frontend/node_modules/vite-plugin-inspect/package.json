{
  "name": "vite-plugin-inspect",
  "type": "module",
  "version": "0.8.4",
  "packageManager": "pnpm@9.0.4",
  "description": "Inspect the intermediate state of Vite plugins",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-inspect#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-inspect.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vite-plugin-inspect/issues"
  },
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt.d.mts",
        "default": "./dist/nuxt.mjs"
      },
      "require": {
        "types": "./dist/nuxt.d.cts",
        "default": "./dist/nuxt.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && run-s build:* && esno ./scripts/postbuild.ts",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && cross-env INSPECT_DEV=true vite src/client",
    "dev:client": "vite build src/client --watch",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0 || ^5.0.0-0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "@rollup/pluginutils": "^5.1.0",
    "debug": "^4.3.4",
    "error-stack-parser-es": "^0.1.1",
    "fs-extra": "^11.2.0",
    "open": "^10.1.0",
    "perfect-debounce": "^1.0.0",
    "picocolors": "^1.0.0",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@antfu/ni": "^0.21.12",
    "@iconify/json": "^2.2.202",
    "@nuxt/kit": "^3.11.2",
    "@types/codemirror": "^5.60.15",
    "@types/debug": "^4.1.12",
    "@types/diff-match-patch": "^1.0.36",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@unocss/eslint-config": "^0.59.4",
    "@unocss/eslint-plugin": "^0.59.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.23",
    "@vueuse/core": "^10.9.0",
    "@vueuse/router": "^10.9.0",
    "bumpp": "^9.4.0",
    "codemirror": "^5.65.16",
    "codemirror-theme-vars": "^0.1.2",
    "comlink": "^4.4.1",
    "cross-env": "^7.0.3",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.5.0",
    "eslint": "^9.1.0",
    "esno": "^4.7.0",
    "fuse.js": "^7.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "ohash": "^1.1.3",
    "prism-theme-vars": "^0.2.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "splitpanes": "^3.1.5",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unocss": "^0.59.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vis-data": "7.1.7",
    "vis-network": "9.1.6",
    "vite": "^5.2.10",
    "vite-dev-rpc": "^0.1.4",
    "vite-hot-client": "^0.2.3",
    "vite-plugin-pages": "^0.32.1",
    "vue": "^3.4.23",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.13"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
